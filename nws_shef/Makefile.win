TARGET=$(OUT_DIR)\javaShefDssLib.lib

all : $(TARGET)

$(TARGET): $(OBJ)
	@ if NOT EXIST $(OUT_DIR) mkdir $(OUT_DIR)
	$(LINK) $** /out:$(TARGET)
	
.f.obj:
    $(FC) $(FFLAGS) $<
	
.for.obj:
    $(FC) $(FFLAGS) $<

clean:
	del *.obj 2>nul
	del $(TARGET) 2>nul