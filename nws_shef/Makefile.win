LINK=xilib
FC=IFORT

!IFDEF DEBUG
D_LEVEL=Full 
OUT_DIR=x64\Debug
DLIB=/dbglibs
!MESSAGE building Debug
!ELSE
D_LEVEL=None
OUT_DIR=x64\Release
DLIB=
!MESSAGE Building Release
!ENDIF


FFLAGS=/nologo /debug:$(D_LEVEL) /MP /Od /fpp /I"../heclib/heclib_f/src/headers" \
 /reentrancy:threaded /warn:noalignments \
 /Qsave /Qinit:zero /names:lowercase /iface:cref /assume:underscore  /traceback \
